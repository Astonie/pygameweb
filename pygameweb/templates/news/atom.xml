{% cache 60*5 %}<?xml version="1.0" encoding="utf-8"?>
<!-- generator="pygameweb 0.0.1" -->
<feed version="0.3" xmlns="http://purl.org/atom/ns#">
    <title>pygame news</title>
    <tagline></tagline>
    <link rel="alternate" type="text/html" href="https://www.pygame.org"/>
    <id>https://www.pygame.org</id>
    <modified>{{ news[0].datetimeon.strftime('%a, %d %b %y %T') }}</modified>
    <generator>pygameweb 0.0.1</generator>
    {% for new in news %}
    <entry>
        <title>{{ new.title }}</title>
        <link rel="alternate" type="text/html" href="https://www.pygame.org/news.html"/>
        <created>{{ new.datetimeon.strftime('%a, %d %b %y %T') }}</created>
        <issued>{{ new.datetimeon.strftime('%a, %d %b %y %T') }}</issued>
        <modified>{{ new.datetimeon.strftime('%a, %d %b %y %T') }}</modified>
        <id>https://www.pygame.org/news.html</id>
        <author>
            <name>pygame.org</name>
        </author>
        <summary>{{ new.description_html }}</summary>
    </entry>
    {% endfor %}
</feed>{% endcache %}